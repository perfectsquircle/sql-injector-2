{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "alpinejs@3.13.0",
      "destination": "wwwroot/lib/alpinejs/"
    },
    {
      "provider": "unpkg",
      "library": "dexie@3.2.4",
      "destination": "wwwroot/lib/dexie/",
      "files": [
        "dist/modern/dexie.min.mjs",
        "dist/modern/dexie.min.mjs.map",
        "dist/modern/dexie.mjs",
        "dist/modern/dexie.mjs.map"
      ]
    },
    {
      "library": "pure@3.0.0",
      "destination": "wwwroot/lib/pure/",
      "files": ["pure.css", "pure.min.css"]
    },
    {
      "library": "font-awesome@4.7.0",
      "destination": "wwwroot\\lib\\font-awesome"
    }
  ]
}
